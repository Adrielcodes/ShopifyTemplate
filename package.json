{
    "name": "shopify-template",
    "version": "1.0.0",
    "description": "Advanced Starter Kit with Shopify integration. A kit designed for agencies and freelancers who are knowledgeable in HTML, CSS, JS and SSGs, who are wanting to deliver any feature a small to medium business would need",
    "main": "index.js",
    "scripts": {
        "build:eleventy": "cross-env ELEVENTY_ENV=PROD eleventy",
        "watch:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
        "start": "run-p watch:*",
        "build": "run-s build:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Adrielcodes/ShopifyTemplate.git"
    },
    "author": "Adriel",
    "license": "ISC",
    "dependencies": {
        "@11ty/eleventy": "^2.0.1",
        "@11ty/eleventy-navigation": "^0.3.5",
        "@11ty/eleventy-plugin-directory-output": "^1.0.1",
        "@shopify/buy-button-js": "^2.4.0",
        "cross-env": "^7.0.3",
        "netlify-plugin-cache": "^1.0.3",
        "dotenv": "^16.3.1",
        "eleventy-plugin-shopify": "^0.1.0",
        "npm-run-all": "^4.1.5"
    }
}
